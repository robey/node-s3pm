{
  "name": "s3pm",
  "version": "1.0.0",
  "description": "publish to (and link from) S3 as a jank node module storage",
  "author": "Robey Pointer <robeypointer@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/robey/node-s3pm.git"
  },
  "scripts": {
    "clean": "rm -rf lib",
    "prepublish": "6to5 --source-maps --out-dir lib src"
  },
  "dependencies": {
    "bluebird": "*",
    "clicolor": "*",
    "s3": "*",
    "source-map-support": "*"
  },
  "devDependencies": {
    "6to5": "*"
  },
  "files": [
    "bin",
    "lib"
  ],
  "bin": {
    "s3pm": "./bin/s3pm"
  }
}
